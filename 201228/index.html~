<script type="text/javascript">
//sets the base url based on secure or non-secure protocol. necessary to play well across all browsers.
//specifically, IE didn't like base href="/", plus that doesn't play well w/ zoovy hosting of SSL domains.
// the check for file allows us to leave this code uncommented while testing locally.

var ac = {
	"baseURL":"",
	"httpsURL":"https://ssl.zoovy.com/s=www.buckeyetoolsupply.com/",
	"httpURL":"http://www.buckeyetoolsupply.com/"
	};

if(document.location.protocol != 'file:')	{
	ac.baseURL  = "https:" == document.location.protocol ? ac.httpsURL : ac.httpURL; 
	document.getElementsByTagName('base')[0].href = ac.baseURL;
	}
</script>
<script type="text/javascript" src="jquery-1.7.2.min.js"></script>
<script type="text/javascript" src="loadscript.js"></script>

<!-- for local testing, use full path. for deplyment, use "jquery/config.js" -->
<script type="text/javascript">
loadScript(ac.baseURL+"_bts12_init.js");
</script>

